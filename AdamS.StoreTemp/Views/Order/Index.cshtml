@model List<AdamS.OnlineStore.DomainModel.Order>

@{
    ViewBag.Title = "title";
}

<h2>Orders</h2>

<table class="table table-striped">
    <tr class="h4">
        <th>OrderId</th>
        <th>OrderTotal</th>
        <th>CustomerId</th>
        <th>FirstName</th>
        @*<th>Delete</th>*@

    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.OrderId</td>
            <td>@order.OrderTotal</td>
            <td>@order.Customer.CustomerId</td>
            <td>@order.Customer.FirstName</td>
            @*<td>@Html.ActionLink("Delete","Delete", new {id=order.OrderId })</td>*@

        </tr>
    }
</table>